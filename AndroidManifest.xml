<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.itunesstoreappslisting"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="19" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:name="com.example.itunesstoreappslisting.ParseApplication"
        android:allowBackup="true"
        android:icon="@drawable/list_all_participants"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.itunesstoreappslisting.LoginActivity"
            android:label="@string/title_activity_login" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.itunesstoreappslisting.SignUpActivity"
            android:label="@string/title_activity_sign_up" >
        </activity>
        <activity
            android:name="com.example.itunesstoreappslisting.AppsActivity"
            android:label="@string/title_activity_apps" >
        </activity>
        <activity
            android:name="com.example.itunesstoreappslisting.PreviewActivity"
            android:label="@string/title_activity_preview" >
        </activity>
        <activity
            android:name="com.example.itunesstoreappslisting.WebViewActivity"
            android:label="@string/title_activity_web_view" >
        </activity>
    </application>

</manifest>
